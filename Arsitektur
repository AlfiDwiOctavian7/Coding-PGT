        function createCookie(name,value,days) {
            var expires = "";
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days*24*60*60*1000));
                expires = "; expires=" + date.toUTCString();
            }
            document.cookie = name + "=" + value + expires + "; path=/";
        }

        function readCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for(var i=0;i < ca.length;i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1,c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
            }
            return null;
        }
   })(LITHIUM.jQuery);

    function stickyScroll(e) {
        var header = document.querySelector('.community-header-component');
        var welcomeHeader = document.querySelector('.custom-profile-completeness-banner');
        var headerBlank = document.querySelector('.community-header-blank');
        var classFixed = 'is-fixed';
        var classDisplay = 'is-display';

        if ( window.pageYOffset > 0) {
            header.classList.add(classFixed);

            if (welcomeHeader) {
                welcomeHeader.classList.add(classFixed);
            }
            headerBlank.classList.add(classDisplay);
        }

        if ( window.pageYOffset === 0) {
            header.classList.remove(classFixed);

            if (welcomeHeader) {
                welcomeHeader.classList.remove(classFixed);
            }
            headerBlank.classList.remove(classDisplay);
        }
    };
    window.addEventListener('scroll', stickyScroll, false);

    var addFollwerEndpoint = '/plugins/custom/microsoft/o365/add-follow-user?tid=6224709546304649859';
    var removeFollwerEndpoint = '/plugins/custom/microsoft/o365/remove-follow-user?tid=6224709546304649859';
    function addFollowUser(currentElement, userId) {
        $.post(
            addFollwerEndpoint + "&followUid=" + userId,
            function( data ) {
                if(data.status === "success"){
                    $(currentElement).parent().html('<a class="button follow-button" tabindex="0" onclick="removeFollowUser(this, \''+userId+'\')">Unfollow</a>');
                } else {
                    console.log(data);
                }
            }
        );
    }

    function addFollowUserBy(event, username, currentElement, userId) {       
        if(event.type === 'click' || (event.type === 'keydown' && event.keyCode === 13)) {
            $.post(
                addFollwerEndpoint + "&followUid=" + userId,
                function( data ) {
                    if(data.status === "success"){
                        $(currentElement).parent().html('<a class="button follow-button unfollow-btn" tabindex="0" role="button" onclick="removeFollowUserBy(event, \''+username+'\', this, \''+userId+'\')" onKeyDown="removeFollowUserBy(event,  \''+username+'\', this, \''+userId+'\')"  aria-label="unfollow \''+username+'\'">Unfollow</a>');
                        setTimeout(function () {
                            $('.unfollow-btn').focus();
                        });
                    } else {
                        console.log(data);
                    }
                }
            );
        }
    }

    function addFollowUserProfile(currentElement, userId) {
    $.post(
        addFollwerEndpoint + "&followUid=" + userId,
        function( data ) {
            if(data.status === "success"){
                $(currentElement).parent().html('<a class="button follow-button margin-right-15" tabindex="0" role="button" onclick="removeFollowUserProfile(this, \''+userId+'\')">Unfollow</a><a class="button message-button" href="/t5/notes/privatenotespage/tab/compose/note-to-user-id/"+userId>Message</a>');
            } else {
                console.log(data);
            }
        }
        );
    }

    function removeFollowUser(currentElement, userId) {
        $.post(
            removeFollwerEndpoint + "&followUid=" + userId,
            function( data ) {
                if (data.status === "success"){
                    $(currentElement).parent().html('<a class="button follow-button" tabindex="0" onclick="addFollowUser(this, \''+userId+'\')">Follow</a>');
                } else {
                    console.log(data);
                }
            }
        );
    }

    function removeFollowUserBy(event, username, currentElement, userId) {
        if(event.type === 'click' || (event.type === 'keydown' && event.keyCode === 13)) {
            $.post(
                removeFollwerEndpoint + "&followUid=" + userId,
                function( data ) {
                    if (data.status === "success"){
                        $(currentElement).parent().html('<a class="button follow-button follow-btn" tabindex="0" onclick="addFollowUserBy(event,  \''+username+'\', this, \''+userId+'\')" onKeyDown="addFollowUserBy(event,  \''+username+'\', this, \''+userId+'\')"  aria-label="follow \''+username+'\'">Follow</a>');
                        setTimeout(function () {
                            $('.follow-btn').focus();
                        });
                    } else {
                        console.log(data);
                    }
                }
            );
        }
    }

    function removeFollowUserProfile(currentElement, userId) {
    $.post(
        removeFollwerEndpoint + "&followUid=" + userId,
        function( data ) {
            if (data.status === "success"){
                $(currentElement).parent().html('<a class="button follow-button margin-right-15" tabindex="0" onclick="addFollowUserProfile(this, \''+userId+'\')">Follow</a><a class="button message-button" href="/t5/notes/privatenotespage/tab/compose/note-to-user-id/"+userId>Message</a>');
            } else {
                console.log(data);
            }
        }
        );
    }

     

            (function($) {
                $(document).ready(function() {
                    // page.name of GroupHubCreatePage does not exist so need this check for now
                    if ($('.lia-quilt-group-hub-create-page').length === 0) {
                        $('input[type=checkbox]').each(function(){
                            if(!$(this).hasClass("lia-add-new-attachment") && !$(this).hasClass("disabled-c-checkbox")) {
                                var inputClone = $(this).clone();
                                var mainDiv = $(document.createElement('div')).addClass("c-checkbox");
                                var label = $(document.createElement('label')).addClass("c-label");
                                var span = document.createElement('span');
                                label.append(inputClone);
                                label.append(span);
                                mainDiv.append(label);
                                $(this).parent().html(mainDiv);
                            }
                        });
                    }
                });
            })(LITHIUM.jQuery);


;(function($) {
$(document).ready(function() {

