	;(function($) {
		$(document).ready(function() {          
            var blogTitle = $('.MessageSubjectIcons').text();  
            $(".lia-media-image").removeAttr("title");

            $(".lia-media-image").each(function(index){
                var imageNumber = index + 1;
                var altText = 'thumbnail image ' + imageNumber + ' of blog post titled ' + blogTitle;

                var imageCaption = $(this).parent('.lia-message-image-wrapper').siblings('.lia-inline-image-caption').text();      
                if(imageCaption != '') {
                    altText = 'thumbnail image ' + imageNumber + ' captioned ' + imageCaption;
                }

                $(this).attr('alt', altText);
                $(this).data('alt', altText);
            });

            //after lingotech 
            $('.lia-media-image').load(function() {
                var altText = $(this).attr('alt');

                if (typeof altText === 'undefined' || altText === false || altText === 'undefined' ) {
                    altText = $(this).data('alt');

                    if (typeof altText !== 'undefined' && altText !== false && altText !== '') {
                        $(this).attr('alt', altText);
                    } else {
                        altText = 'thumbnail image of blog post titled ' + blogTitle;

                        var imageCaption = $(this).parent('.lia-message-image-wrapper').siblings('.lia-inline-image-caption').text();      
                        if(imageCaption != '') {
                            altText = 'thumbnail image captioned ' + imageCaption;
                        }

                        $(this).attr('alt', altText);
                        $(this).data('alt', altText);
                    }
                }
            });

            $('.BlogArticlePage').on('focus', '.lia-media-image-frame' ,function () {     
                $('.lia-media-image-frame img').attr('title', 'thumbnail image preview for blog post titled ' + blogTitle);
            });

            $('.lia-media-image').on('keypress click', function (e) {
                clickedImage = $(this);  
                UTILITIES.accessibleAlert('pageLevel', 'Opened image modal'); 
                if (e.type === 'keypress' && e.keyCode === 13) {
                   $(this).click();   
                }   
            });
            
            $('.BlogArticlePage').on('keyup click', '.lia-lightbox-close-button, .lia-modal-window ' ,function (e) { 
                UTILITIES.accessibleAlert('pageLevel', 'closed image modal');
                if(e.type === 'click' || e.keyCode === 13 || e.keyCode === 27) {     
                    e.preventDefault();  
                    setTimeout(function () {
                        clickedImage.focus();
                    });
                }
		    }); 

