    ;(function($) {
    $(document).ready(function() {
        $('.custom-community-hubs-menu-content button').on('click', function(ev) {
            var key = $(this).attr('data-key');
            var browseAllText = 'Browse All Community Hubs';
            ev.stopPropagation();

            $('.menu .active').removeClass("active");
            $(this).parent().addClass("active");
            $('.dataBlock').removeClass("visible");
            $('.dataBlock[data-key=' + key + ']').addClass("visible");
            $("#browse-all-communities").attr("href", "/t5/Communities/ct-p/communities").find('span').text(browseAllText);

            if (key === 'grouphubs') {
                if ($('#lia-body').hasClass("lia-user-status-registered")) {
                    browseAllText = 'Browse My Group Hubs';
                    $("#browse-all-communities").attr("href", "/t5/grouphubs/page/user-id/-1").find('span').text(browseAllText);
                } else if($('#lia-body').hasClass("lia-user-status-anonymous")) {
                    browseAllText = 'Browse All Group Hubs';
                    $("#browse-all-communities").attr("href", "/t5/custom/page/page-id/GroupHubs").find('span').text(browseAllText);
                }
            } else if (key ==='VideoHub') {
                browseAllText = 'See All Videos';
                $("#browse-all-communities").attr("href", "/t5/video-hub/ct-p/VideoHub").find('span').text(browseAllText);
            }
        });

        $('.custom-community-hubs-menu-wrapper, #browse-all-communities').on('click', function(ev) {
            ev.stopPropagation();
        });

        $('#close-icon').on('click', function(ev) {
            $('.community-menu-item.community-menu-dropdown.show').removeClass("show");
            $('.dropdown.dropdown-events.dropdown-primary-nav.show').removeClass("show");
        });

        if ($('.lia-quilt-category-page-communities').length !== 0) {
            $('#communities_home').on('click', function(ev){
                ev.stopPropagation();
                $('#category-filter-button-all').click();
            });

            $('#communities_products').on('click', function(ev){
                ev.stopPropagation();
                $('#category-filter-button-products-services').click();
            });

            $('#communities_special').on('click', function(ev){
                ev.stopPropagation();
                $('#category-filter-button-solutions').click();
            });
        }
    });
