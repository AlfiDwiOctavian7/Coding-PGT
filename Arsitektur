    ;(function($) {
        $(function() {
            if ($('body').hasClass('lia-blog')) {

                setTimeout(function() {
                    var isChecked = $('#lia-publicationScheduled:checked').length;

                    if (isChecked > 0) {
                        $('#lia-scheduledPublicationDate_date, #lia-scheduledPublicationDate_time').prop('disabled', false);
                        $('input.lia-button-Submit-action').addClass('lia-link-disabled').prop('disabled', 'disabled')
                    } else {
                        $('#lia-scheduledPublicationDate_date, #lia-scheduledPublicationDate_time').prop('disabled', true);
                        $('.lia-button-Submit-action').removeClass('lia-link-disabled').removeAttr('disabled')
                    }

                    $('#lia-publicationScheduled').change(function() {

                        if ($(this).prop('checked')) {
                            $('#lia-scheduledPublicationDate_date, #lia-scheduledPublicationDate_time').prop('disabled', false);
                            $('input.lia-button-Submit-action').addClass('lia-link-disabled').prop('disabled', 'disabled')
                        } else {
                            $('#lia-scheduledPublicationDate_date, #lia-scheduledPublicationDate_time').prop('disabled', true);
                            $('.lia-button-Submit-action').removeClass('lia-link-disabled').removeAttr('disabled')
                        }
                    });
                }, 2000);
            }
        });
    })(LITHIUM.jQuery);

    ;(function($) {
        $(document).ready(function () {
            // accessiblity changes
            var customBreadCrumbs = function () {
                var homeLink;
                var navMarkup = '<nav role="navigation" aria-label="Your location in the site" class="BreadCrumb crumb-line lia-breadcrumb lia-component-common-widget-breadcrumb">';
                var finalCrumb;
                var pageName = 'BlogArticlePage';

                $('.crumb-line').removeAttr('aria-label');
                $('.final-crumb').attr('aria-current', 'true');
                $('div.crumb-line').replaceWith(navMarkup + $('div.crumb-line').html());

                homeLink = $($('.custom-common-widget-breadcrumb-wrapper .crumb-line').children('li')).get(0);

                if (homeLink) {
                    $('.custom-common-widget-breadcrumb-wrapper > li').get(0).remove();
                    $('.crumb-line ul').before(homeLink);
                } else if (pageName === 'GroupHubPage') {
                    $('.frag li').insertAfter($('.crumb-line li').eq(0));
                    $('.custom-common-widget-breadcrumb-wrapper > *').not('nav').remove();
                } else if (pageName === 'mct-central-video-page') {
                    $('.frag-custom-page li').insertBefore('li.lia-breadcrumb-node.crumb.final-crumb');
                    $('.custom-common-widget-breadcrumb-wrapper > *').not('nav').remove();
                } else {
                    $('.custom-common-widget-breadcrumb-wrapper > *').not('nav').remove();
                }


                finalCrumb = $('.final-crumb').eq(0).text().trim();
                $('.final-crumb span').attr('aria-label', 'current location is ' + finalCrumb)
                    .attr('title', 'current location is ' + finalCrumb);

                $('.lia-breadcrumb-seperator').remove();
                $('.custom-common-widget-breadcrumb-wrapper').removeClass('hidden');
            };

            customBreadCrumbs();
        });
    })(LITHIUM.jQuery);

