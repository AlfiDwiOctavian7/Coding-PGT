			$('.community-header-wrapper .community-menu-wrapper').addClass('community-menu-hidden');
            $('.community-header-wrapper .community-menu-wrapper').removeClass('community-menu-shown');
            $('#community-menu-wrapper-menu-link').focus();
			$('html').removeClass("remove-scroll-page");
		}
	}

	});
	
