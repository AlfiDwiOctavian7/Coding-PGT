	LITHIUM.jQuery("body").unbind("click").click(function(e){
		if (!menuInitRemoved) {
			$('.community-header-wrapper .community-menu-wrapper').removeClass('community-menu-hidden-init');
			menuInitRemoved = true;
		}
	var elementId = $(e.target).attr('id');
	var showMenu = false;
	if(elementId != null) {
		var index = elementId.indexOf('community-menu-wrapper');
		if(index != -1) {
		showMenu = true;
		}
	}

	if (showMenu) {
		$('.community-header-wrapper .community-menu-wrapper').addClass('community-menu-shown');
        $('.tab-homepage a').focus();
		$('.community-header-wrapper .community-menu-wrapper').removeClass('community-menu-hidden');
        $('html').addClass("remove-scroll-page");
	} else { //hide
